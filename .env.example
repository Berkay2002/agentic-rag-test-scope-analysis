# LangSmith Configuration (Optional - for observability)
LANGCHAIN_TRACING_V2=true
LANGCHAIN_API_KEY=your_langsmith_api_key_here
LANGCHAIN_PROJECT=agrag-test-scope-analysis

# Google Generative AI (Required)
GOOGLE_API_KEY=your_google_api_key_here
GOOGLE_MODEL=gemini-2.5-flash-latest
GOOGLE_EMBEDDING_MODEL=models/embedding-001
GOOGLE_THINKING_LEVEL=low  # optional: low/high, Gemini 3 reasoning depth
GOOGLE_THINKING_BUDGET=256  # optional: token budget for Gemini 2.5 thinking

# Neo4j Configuration (Required)
# For Neo4j Aura (cloud): use neo4j+s:// protocol
# For local Neo4j: use bolt://localhost:7687
NEO4J_URI=neo4j+s://your_instance_id.databases.neo4j.io
NEO4J_USERNAME=neo4j
NEO4J_PASSWORD=your_neo4j_password_here
NEO4J_DATABASE=neo4j
AURA_INSTANCEID=your_instance_id
AURA_INSTANCENAME=your_instance_name

# PostgreSQL/Neon Configuration (Required)
# Option 1: Use Neon connection string
NEON_CONNECTION_STRING=postgresql://user:password@host:5432/dbname?sslmode=require

# Option 2: Use individual PostgreSQL settings
# POSTGRES_HOST=localhost
# POSTGRES_PORT=5432
# POSTGRES_USER=postgres
# POSTGRES_PASSWORD=your_postgres_password_here
# POSTGRES_DATABASE=agrag

# Agent Configuration (Optional - has defaults)
MAX_TOOL_CALLS=10
MAX_MODEL_CALLS=20
AGENT_TEMPERATURE=0.0

# Vector Search Configuration (Optional - has defaults)
VECTOR_SEARCH_SIMILARITY_THRESHOLD=0.7
GRAPH_TRAVERSAL_MAX_DEPTH=3
EMBEDDING_DIMENSIONS=768
